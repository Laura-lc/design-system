{
  "name": "@cmsgov/medicare-site-package",
  "version": "0.1.1",
  "description": "A 'site package' on top of the CMSGOV Design System. Consists of SCSS overrides and React components for use on Medicare.gov websites.",
  "license": "CC0-1.0",
  "scripts": {
    "build-css": "sass -I node_modules/ -s compressed src/index.scss dist/index.min.css",
    "build-js": "./node_modules/.bin/webpack --mode=production --config Webpack.config.js",
    "build": "yarn build-css && yarn build-js",
    "test-css": "sass -I node_modules/ src/index.scss dist/index.css",
    "watch": "yarn watch-css &>/dev/null & yarn watch-js",
    "watch-css": "yarn test-css --watch",
    "watch-js": "./node_modules/.bin/webpack -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.cms.gov/MedicareGov/ds-site-package.git"
  },
  "keywords": [
    "design-system",
    "medicare.gov",
    "site-package"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "author": "Ad Hoc MCT Team",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.6",
    "@types/lodash": "^4.14.138",
    "@types/react": "^16.4.18",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "sass": "^1.14.3",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@cmsgov/design-system-core": "^3.5.1",
    "@cmsgov/design-system-layout": "^3.5.1",
    "@cmsgov/design-system-support": "^3.5.1",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15"
  }
}
